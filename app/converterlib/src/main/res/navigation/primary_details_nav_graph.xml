<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/item_list_fragment">

    <fragment
        android:id="@+id/item_list_fragment"
        android:name="com.example.converterlib.ui.RateListFragment"
        android:label="All Rates"
        tools:layout="@layout/fragment_rate_list">
        <action
            android:id="@+id/action_nav_rateListFrg_to_rateConverterFrg"
            app:destination="@id/item_detail_fragment" />
    </fragment>
    <fragment
        android:id="@+id/item_detail_fragment"
        android:name="com.example.converterlib.ui.RateConverterFragment"
        android:label="Rates Converter"
        tools:layout="@layout/fragment_rate_converter"></fragment>
